[alias]
	toplevel = rev-parse --show-toplevel
	
[credential]
	helper = git-credential-manager-core
	credentialStore = plaintext
	
[credential "https://dev.azure.com"]
	useHttpPath = true
	
[user]
	name = root
	email = root@example.com

[include]
  path = /etc/.gitconfig

[init]
	defaultBranch = main

