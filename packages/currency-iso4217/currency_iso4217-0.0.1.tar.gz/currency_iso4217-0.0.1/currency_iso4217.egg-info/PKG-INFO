        >>> [{'id': 41, 'country': 'Chile', 'currency': 'Unidad de fomento', 'decimals': '4'}, {'id': 42, 'country': 'Chile', 'currency': 'Peso chileno', 'decimals': '0'}]
        
        >>> search_by_currency('Peso chileno')
        >>> [{'id': 42, 'country': 'Chile', 'currency': 'Peso chileno', 'decimals': '0'}]
        
