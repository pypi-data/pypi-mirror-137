{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "DICE report schema",
  "type": "object",
  "properties": {
    "info": {
      "type": "object",
      "properties": {
        "CO2MPAS_version": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^\\s*4.1.10\\s*$"

            },
            {
              "type": "null"
            }
          ]
        },
        "JET_version": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^\\s*1.0.16\\s*$"
            },
            {
              "type": "null"
            }
          ]
        },
        "SCHEMA_version": {
          "oneOf": [
            {
              "type": "string",
              "pattern": "^\\s*1.2.3\\s*$"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "additionalProperties": true,
      "required": [
        "CO2MPAS_version"
      ]
    },
    "deviation": {
      "type": "object",
      "properties": {
        "nedc_h": {
          "type": "number"
        },
        "wltp_h": {
          "type": "number"
        },
        "nedc_l": {
          "type": "number"
        },
        "wltp_l": {
          "type": "number"
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": true,
  "required": [
    "info"
  ]
}