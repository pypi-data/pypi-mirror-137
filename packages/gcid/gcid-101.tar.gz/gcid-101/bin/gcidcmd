#!/usr/bin/env python3
# This file is placed in the Public Domain.


"OTP-CR-117/19"


import sys


from gcid.hdl import Handler
from gcid.krn import Cfg, kcmd


import gcid.all


Cfg.wd = "/var/lib/gcid/"


class CLI(Handler):

    def raw(self, txt):
        print(txt)


def main():
    c = CLI()
    kcmd(c, " ".join(sys.argv[1:]))

main()
